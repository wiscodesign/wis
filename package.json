{
  "private": true,
  "name": "wis",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "dev": "rsbuild dev --open",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "dev:alpha": "rsbuild dev --open --env-mode alpha",
    "build:alpha": "rsbuild build --env-mode alpha",
    "dev:beta": "rsbuild dev --open --env-mode beta",
    "build:beta": "rsbuild build --env-mode beta",
    "dev:rc": "rsbuild dev --open --env-mode rc",
    "build:rc": "rsbuild build --env-mode rc",
    "check": "biome check --write",
    "format": "biome format --write",
    "changelog": "pnpm changeset",
    "version": "pnpm run docgen && pnpm changeset version",
    "alpha": "pnpm changeset pre enter alpha && pnpm changeset version && pnpm changeset pre exit",
    "beta": "pnpm changeset pre enter beta && pnpm changeset version && pnpm changeset pre exit",
    "rc": "pnpm changeset pre enter rc && pnpm changeset version && pnpm changeset pre exit",
    "release": "pnpm changeset tag && git push --follow-tags",
    "docgen": "typescript-docgen -s ./src/packages -o ./src/definitions.ts && biome format --write",
    "token": "token -n w -s ./src/assets/wis.basic.tokens.json -t blue:./src/assets/wis.blue.tokens.json,dark:./src/assets/wis.dark.tokens.json,orange:./src/assets/wis.orange.tokens.json -o ./src/themes"
  },
  "lint-staged": {
    "**/*.{ts,tsx,json}": "biome format --write"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "1.1.1",
    "@radix-ui/react-collapsible": "1.1.1",
    "@radix-ui/react-context-menu": "2.2.2",
    "@radix-ui/react-dropdown-menu": "2.1.2",
    "@radix-ui/react-toggle": "1.1.0",
    "@radix-ui/react-toggle-group": "1.1.0",
    "@radix-ui/react-tooltip": "1.1.4",
    "@wisdesign/lsicon": "0.0.19",
    "classnames": "2.5.1",
    "lodash.debounce": "4.0.8",
    "nanoid": "5.0.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "wiscore": "0.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.5",
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.3",
    "@rsbuild/core": "1.2.13",
    "@rsbuild/plugin-react": "1.1.0",
    "@rsbuild/plugin-sass": "1.2.2",
    "@rsbuild/plugin-basic-ssl": "1.1.1",
    "@types/lodash.debounce": "4.0.9",
    "@types/node": "22.13.7",
    "@types/react": "18.3.16",
    "@types/react-dom": "18.3.5",
    "@wisdesign/cli": "0.0.29",
    "@wisdesign/toolkit": "0.0.7",
    "@wisdesign/wis-plugin": "0.0.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "typescript": "5.7.3"
  }
}
