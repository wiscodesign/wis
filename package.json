{
  "private": true,
  "name": "wis",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow npm",
    "prepare": "husky",
    "dev": "wis dev",
    "build": "wis build",
    "token": "token -n w -l ./src/assets/light.json -d ./src/assets/dark.json -o ./src/packages/style/tokens"
