{
  "private": true,
  "name": "wis",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow npm",
    "prepare": "husky",
    "dev": "wis dev",
    "build": "wis build",
    "token": "token -n w -s ./src/assets/wis.basic.tokens.json -t blue:./src/assets/wis.blue.tokens.json,dark:./src/assets/wis.dark.tokens.json,highContrast:./src/assets/wis.highContrast.tokens.json -o ./src/themes"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "prettier --write",
      "eslint"
    ],
    "**/*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.16.7",
    "@babel/runtime-corejs3": "7.16.7",
    "@radix-ui/react-toggle": "1.1.0",
    "@radix-ui/react-toggle-group": "1.1.0",
    "classnames": "2.5.1",
    "i18next": "23.15.1",
    "i18next-browser-languagedetector": "8.0.0",
    "nanoid": "5.0.7",
    "prop-types": "15.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.3",
    "@wisdesign/cli": "0.0.13",
    "@wisdesign/eslint-config": "0.0.1",
    "@wisdesign/plugin-less": "0.0.1",
    "@wisdesign/prettier-config": "0.0.1",
    "@wisdesign/toolkit": "0.0.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.7"
  }
}
