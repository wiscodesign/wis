    "@wisdesign/cli": "0.0.16",
    "@wisdesign/eslint-config": "0.0.1",
    "@wisdesign/prettier-config": "0.0.1",
    "@wisdesign/toolkit": "0.0.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.7"
  }
